<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="265.0" prefWidth="245.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Control.LobbyAppController">
   <children>
      <Pane prefHeight="427.0" prefWidth="313.0" />
      <ScrollPane fx:id="scrollPaneImagenes" prefHeight="427.0" prefWidth="313.0" AnchorPane.leftAnchor="0.0">
         <content>
            <AnchorPane fx:id="anchorPaneImagenes" prefHeight="425.0" prefWidth="309.0">
               <children>
                  <ImageView fx:id="imagenViewNews" fitHeight="318.0" fitWidth="232.0" layoutX="11.0" layoutY="88.0" pickOnBounds="true" preserveRatio="true" />
                  <ImageView fx:id="iconoperfil" fitHeight="27.0" fitWidth="30.0" layoutX="2.0" layoutY="2.0" onMouseClicked="#abrirPerfil" pickOnBounds="true" preserveRatio="true" />
                  <Label fx:id="nombrePerfil" layoutX="39.0" layoutY="7.0" onMouseClicked="#abrirPerfil" prefHeight="17.0" prefWidth="142.0" text="Nombre perfil actual" />
                  <Label layoutX="85.0" layoutY="41.0" text="News">
                     <font>
                        <Font size="29.0" />
                     </font>
                  </Label>
                  <Button layoutX="257.0" layoutY="225.0" mnemonicParsing="false" onAction="#AgregarImagenNueva" prefHeight="6.0" prefWidth="33.0" text="+" textAlignment="JUSTIFY">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Button>
                  <Button fx:id="botonAnteriorImagen" layoutX="254.0" layoutY="88.0" mnemonicParsing="false" onAction="#handleButtonActionAnteriorImagen" prefHeight="44.0" prefWidth="47.0" text="▲" textAlignment="JUSTIFY">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Button>
                  <Button fx:id="botonSiguienteImagen" layoutX="254.0" layoutY="362.0" mnemonicParsing="false" onAction="#handleButtonActionSiguienteImagen" prefHeight="44.0" prefWidth="47.0" text="▼" textAlignment="JUSTIFY">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Button>
               </children></AnchorPane>
         </content>
      </ScrollPane>
   </children>
</AnchorPane>
